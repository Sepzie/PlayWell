{
  "name": "playwell",
  "version": "1.0.0",
  "description": "Game time tracking application",
  "main": "electron.main.js",
  "scripts": {
    "dev:renderer": "vite",
    "dev:electron": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "dev": "concurrently -k \"npm run dev:renderer\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "build:renderer": "vite build",
    "build": "npm run build:renderer && electron-builder",
    "start": "electron .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "framer-motion": "^12.23.22",
    "chart.js": "^4.5.0",
    "electron": "^28.0.0",
    "prisma": "^5.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.52.3",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.0",
    "electron": "^38.2.0",
    "electron-builder": "^24.0.0",
    "vite": "^5.0.0",
    "wait-on": "^7.0.0"
  },
  "keywords": [
    "electron",
    "react",
    "game-tracking",
    "time-management"
  ],
  "author": "",
  "license": "MIT"
}
